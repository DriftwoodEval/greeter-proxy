{
	"name": "greeter-proxy",
	"version": "0.0.0",
	"description": "A TypeScript SMS proxy service connecting Evaluators and Greeters via Twilio and SQLite.",
	"main": "dist/server.js",
	"scripts": {
		"build": "tsc",
		"start": "node dist/server.js",
		"dev": "nodemon --exec ts-node src/server.ts",
		"typecheck": "tsc --noEmit",
		"admin": "ts-node src/admin.ts"
	},
	"license": "ISC",
	"packageManager": "pnpm@10.26.0",
	"pnpm": {
		"onlyBuiltDependencies": [
			"better-sqlite3",
			"lefthook"
		]
	},
	"dependencies": {
		"better-sqlite3": "^12.5.0",
		"body-parser": "^2.2.1",
		"commander": "^14.0.2",
		"dotenv": "^17.2.3",
		"express": "^5.2.1",
		"libphonenumber-js": "^1.12.33",
		"twilio": "^5.11.1"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"@commitlint/cli": "^20.2.0",
		"@commitlint/config-conventional": "^20.2.0",
		"@types/better-sqlite3": "^7.6.13",
		"@types/body-parser": "^1.19.6",
		"@types/express": "^5.0.6",
		"@types/node": "^25.0.3",
		"commitlint": "^20.2.0",
		"lefthook": "^2.0.13",
		"nodemon": "^3.1.11",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3"
	}
}
